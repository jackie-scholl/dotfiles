[pull]
	rebase = true
[user]
	email = jackie.h.scholl@gmail.com
	name = Jackie Scholl
	rebase = true
[init]
	defaultBranch = main
[merge]
	renamelimit = 10000
[push]
	autoSetupRemote = true
[rerere]
	enabled = true
[maintenance]
	repo = /home/jackie/mercury/mercury-web-backend


[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true    # use n and N to move between diff sections
    light = true      # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
    hyperlinks = true
    # hyperlinks-file-link-format = "file://{path}:{line}"

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default

