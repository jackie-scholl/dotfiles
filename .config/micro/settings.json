{
    "colorcolumn": 120,
    "colorscheme": "bubblegum",
    "lsp.server": "haskell=haskell2-language-server-wrapper --lsp -l .devel-logs/hls.log,python=pyls,go=gopls,typescrizz=typescript-language-server --stdio --tsserver-log-file",
    "lsp.tabcompletion": false,
    "mkparents": true,
    "parsecursor": true,
    "rmtrailingws": true,
    "saveundo": true,
    "softwrap": true,
    "tabmovement": true,
    "tabsize": 2,
    "tabstospaces": true
}
