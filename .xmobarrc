Config {
	  font = "xft:Hack-12:Bold"
	, borderColor = "black"
	, border = BottomB
	, pickBroadest = True
	, persistent = True
	, bgColor = "white"
	, fgColor = "black"
	, alpha = 255
	, position = TopW L 100
	, sepChar = "%"
	, alignSep = "}{"
	, commands = [ 
	    Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
    , Run MultiCpu ["-t","<autovbar>"] 10
	  , Run TopProc ["-t","<name1> <cpu1> <mem1>"] 10
	  , Run DiskIO [("/", "<read>/<write>")] [] 10
	  , Run Memory ["-t","Mem: <usedratio>%"] 10
	  , Run Swap [] 10
	  , Run DynNetwork ["-t","Network: <rxvbar> D;<txvbar> U","-S","True","-w","6"] 10
	  , Run Wireless "wlp170s0" [] 30
	  , Run Volume "default" "Master" [] 10
	  , Run Battery [] 30
	  , Run Locks
	  , Run StdinReader
	  , Run Date "%a %b %_d %Y %-I:%M:%S %p" "date" 10
		, Run Weather "KSEA" ["-t","<tempF>F <windMph><windCardinal>  <skyCondition>","-L","40","-H","80","--normal","green","--high","red","--low","lightblue"] 36000
  --, Run Com "fish" ["/home/jackie/Documents/gcal-status/gs.fish"] "agenda" 600
		, Run Com "/home/jackie/.local/bin/gcal-haskell-exe" [] "agenda" 600
	]
	, template = "%cpu% - %multicpu% | %memory% | %default:Master% | %battery% | %locks% | %wi% | %agenda%}  %StdinReader% {<fc=#ee9a00>%date%</fc> | %KSEA% Seattle  "
	, lowerOnStart = False
	}
